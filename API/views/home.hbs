<h3 class="mb-4">Users table: </h3>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">ID</th>
            <th scope="col">Username</th>
            <th scope="col">Email</th>
            <th scope="col">Name</th>
            <th scope="col">Password</th>
            <th scope="col">Quizz</th>
            <th scope="col">Box(s)</th>
            <th scope="col">x</th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
        <tr>
            <td>{{@index}}</td>
            <td>{{this._id}}</td>
            <td>{{this.username}}</td>
            <td>{{this.email}}</td>
            <td>{{this.fullname}}</td>
            <td id="pwd"><span id="hidePwd">****</span> <span style="cursor:pointer;" id="showPwd"
                    onclick="showPwd(this, '{{this.password}}')"><svg xmlns="http://www.w3.org/2000/svg"
                        xmlns:xlink="http://www.w3.org/1999/xlink" width="24" height="18" viewBox="0 0 24 24">
                        <image id="oeil" width="24" height="24"
                            xlink:href="data:image/png;base64," />
                    </svg>
                </span></td>
            <td>{{#haveQuizz this.quizz}} {{this}} {{/haveQuizz}}</td>
            <td>{{this.box}}</td>
            <td class="actions"><a class="btn btn-white-pink" target="_blank" href="users/id/{{this._id}}">JSON</a><a
                    class="btn btn-yellow" href="users/edit/{{this._id}}">E</a><a class="btn btn-bordeaux"
                    href="users/delete/{{this._id}}">D</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>

<a href="users/add" class="btn btn-orange">Register new user</a>
<a href="users/drop" class="btn btn-yellow">Drop</a>

<div style="height:5vh"></div>

