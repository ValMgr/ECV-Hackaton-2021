<script>
    let s = false;
    function showPwd(el, pwd) {
        s = !s;
        if (s) {
            el.parentNode.querySelector('#hidePwd').innerText = pwd;
        }
        else {

            el.parentNode.querySelector('#hidePwd').innerText = "****";
        }
    }
</script>